#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for (int i=1;i<=t;i++)
	{
	    int x,y,z;
	    cin>>x>>y>>z;
	     
	     int totalmoney = (5*x)+(10*y);
	     
	     int maxchoclates = totalmoney/z;
	     
	     cout<< maxchoclates<<endl;
	}
	return 0;
}
