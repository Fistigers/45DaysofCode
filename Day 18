#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	
	for(int i=1;i<=t;i++){
	    
	    int a,b;
	    cin>>a>>b;
	    
	    if(a>b){
	        cout<<7-a<<endl;
	    }
	    else{
	        cout<<7-b<<endl;
	    }
	}
	return 0;
}
